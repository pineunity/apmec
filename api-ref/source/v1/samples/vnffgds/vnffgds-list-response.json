                                "properties": {
                                    "vendor": "tacker",
                                    "connection_point": [
                                        "CP12",
                                        "CP22"
                                    ],
                                    "version": 1.0,
                                    "constituent_vnfs": [
                                        "VNFD1",
                                        "VNFD2"
                                    ],
                                    "number_of_endpoints": 2,
                                    "dependent_virtual_link": [
                                        "VL12",
                                        "VL22"
                                    ]
                                }
                            }
                        }
                    }
                }
            },
            "id": "60b8e717-11a7-4395-8438-a21d2eeeb855"
        }
    ]
