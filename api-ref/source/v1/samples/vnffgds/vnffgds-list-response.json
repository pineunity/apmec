                    ],
                    "description": "Sample VNFFG template",
                    "topology_template": {
                        "node_templates": {
                            "Forwarding_path1": {
                                "type": "tosca.nodes.nfv.FP.Tacker",
                                "description": "creates path (CP12->CP22)",
                                "properties": {
                                    "policy": {
                                        "type": "ACL",
                                        "criteria": [
                                            {
                                                "network_src_port_id": "5fad003e-95c0-46b3-a137-4d423b6ab958"
                                            },
                                            {
                                                "destination_port_range": "80-1024"
